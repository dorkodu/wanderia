{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "Bundler",
    "noEmit": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules"
  ]
}